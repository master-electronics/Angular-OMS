    "gql:codegen": "graphql codegen",
    "gql:coverage": "graphql coverage"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^15.0.1",
    "@angular/animations": "^15.0.1",
    "@angular/common": "^15.0.1",
    "@angular/compiler": "^15.0.1",
    "@angular/core": "^15.0.1",
    "@angular/forms": "^15.0.1",
    "@angular/localize": "^15.0.1",
    "@angular/platform-browser": "^15.0.1",
    "@angular/platform-browser-dynamic": "^15.0.1",
    "@angular/router": "^15.0.1",
    "@ant-design/icons-angular": "^15.0.0",
    "angular-calendar": "^0.30.1",
    "apollo-angular": "^4.1.1",
    "date-fns": "^2.29.2",
    "ng-zorro-antd": "^14.3.0",
    "rxjs": "^7.5.6",
    "simple-keyboard": "^3.4.178",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.3",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular/cli": "^15.0.1",
    "@angular/compiler-cli": "^15.0.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.6",
