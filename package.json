{
  "name": "ag-wms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration=dev",
    "watch": "ng build --watch --configuration development",
    "cypress:run": "cypress run --record --key 44dfdd28-8118-4667-832f-96dcd359110d",
