{
  "name": "ag-wms",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build:local": "NODE_ENV=production ng build --configuration=local && sudo docker-compose up --build -d",
    "build:stagging": "NODE_ENV=production ng build --configuration=d1 && sudo docker-compose up --build -d",
    "build:prod": "NODE_ENV=production ng build --configuration=p1 && sudo docker-compose up --build -d",
    "gql": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config GQLmerp.yml",
    "gql:codegen": "graphql codegen",
    "gql:coverage": "graphql coverage",
    "gql:validate": "graphql validate",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.3",
    "@angular/core": "^11.2.3",
    "@angular/forms": "^11.2.3",
    "@angular/platform-browser": "^11.2.3",
    "@angular/router": "^11.2.3",
    "@apollo/client": "^3.3.21",
    "angular-google-tag-manager": "^1.4.2",
    "angular-ng-autocomplete": "^2.0.5",
    "apollo-angular": "^2.6.0",
    "aws-amplify": "^3.4.3",
    "core-js": "^3.15.2",
    "graphql": "^15.5.1",
    "idle-timer-manager": "^0.0.5",
    "ng-keyboard-shortcuts": "^10.1.17",
    "ngx-cookie-service": "^11.0.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0",
    "@angular-devkit/build-angular": "^0.1102.14",
    "@angular/cli": "^11.2.14",
    "@angular/compiler": "^11.2.6",
    "@angular/compiler-cli": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.3",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/near-operation-file-preset": "^1.18.4",
    "@graphql-codegen/typescript": "^1.22.4",
    "@graphql-codegen/typescript-apollo-angular": "2.3.2",
    "@graphql-codegen/typescript-document-nodes": "1.17.10",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-eslint/eslint-plugin": "^0.9.1",
    "@ngneat/tailwind": "^6.0.3",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/line-clamp": "0.2.0",
    "@tailwindcss/typography": "0.4.0",
    "@types/node": "^14.17.5",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "autoprefixer": "^10.3.0",
    "cypress": "^7.7.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.5",
    "postcss-ng-tailwind-in-components": "0.0.3",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0",
    "prettier-plugin-tailwind": "^2.2.12",
    "protractor": "~7.0.0",
    "tailwindcss": "2.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.6",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md,html}": "prettier --write"
  },
  "browser": {
    "crypto": false
  }
}
